{
  "name": "@foo-dog/attrs",
  "version": "0.2.2",
  "description": "Run attributes",
  "source": "src/index.js",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "type": "module",
  "scripts": {
    "build": "parcel build",
    "postversion": "git push && git push --tags",
    "test": "tap"
  },
  "keywords": [
    "aakoch",
    "pug",
    "foo-dog",
    "foodog"
  ],
  "author": "Adam Koch (aakoch)",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "^0.1.2",
    "parcel": "^2.0.1",
    "tap": "^15.1.5"
  },
  "dependencies": {
    "@aakoch/generator": "file:../generator",
    "@foo-dog/utils": "^0.2.0",
    "chalk": "^4.1.2",
    "debug": "^4.3.3"
  },
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "@istanbuljs/esm-loader-hook"
    ]
  },
  "engines": {
    "node": ">= 16.9.1",
    "npm": ">= 7.21.1"
  },
  "targets": {
    "main": {
      "optimize": true,
      "isLibrary": true
    },
    "module": {
      "optimize": true,
      "isLibrary": true
    }
  },
  "exports": {
    "import": "./dist/module.js",
    "require": "./dist/main.js"
  }
}
